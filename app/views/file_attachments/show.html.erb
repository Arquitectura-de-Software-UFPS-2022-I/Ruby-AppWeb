<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @file_attachment.name %>
</p>

<%= content_tag(:iframe, '', src: "data:application/pdf;base64,#{@file_attachment.get_base_64}", 
                   width: 800, height: 500, frameborder: 0) %>

<p>
  <strong>Created date:</strong>
  <%= @file_attachment.created_date %>
</p>

<%= link_to 'Edit', edit_file_attachment_path(@file_attachment) %> |
<%= link_to 'Back', file_attachments_path %>
