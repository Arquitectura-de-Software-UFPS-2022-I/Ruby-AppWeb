<p id="notice"><%= notice %></p>

<h1>Signature Requests</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Document</th>
      <th>User</th>
      <th>Created date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @signature_requests.each do |signature_request| %>
      <tr>
        <td><%= signature_request.subject %></td>
        <td><%= signature_request.document %></td>
        <td><%= signature_request.user %></td>
        <td><%= signature_request.created_date %></td>
        <td><%= link_to 'Show', signature_request %></td>
        <td><%= link_to 'Edit', edit_signature_request_path(signature_request) %></td>
        <td><%= link_to 'Destroy', signature_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Signature Request', new_signature_request_path %>
