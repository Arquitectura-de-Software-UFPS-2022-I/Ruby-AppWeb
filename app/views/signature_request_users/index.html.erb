<p id="notice"><%= notice %></p>

<h1>Signature Request Users</h1>

<table>
  <thead>
    <tr>
      <th>Request</th>
      <th>User</th>
      <th>Pos x</th>
      <th>Pos y</th>
      <th>Num page</th>
      <th>Signed</th>
      <th>Signature date</th>
      <th>Created date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @signature_request_users.each do |signature_request_user| %>
      <tr>
        <td><%= signature_request_user.request %></td>
        <td><%= signature_request_user.user %></td>
        <td><%= signature_request_user.pos_x %></td>
        <td><%= signature_request_user.pos_y %></td>
        <td><%= signature_request_user.num_page %></td>
        <td><%= signature_request_user.signed %></td>
        <td><%= signature_request_user.signature_date %></td>
        <td><%= signature_request_user.created_date %></td>
        <td><%= link_to 'Show', signature_request_user %></td>
        <td><%= link_to 'Edit', edit_signature_request_user_path(signature_request_user) %></td>
        <td><%= link_to 'Destroy', signature_request_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Signature Request User', new_signature_request_user_path %>
